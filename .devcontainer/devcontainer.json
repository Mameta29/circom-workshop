{
    "name": "Circom Workshop Environment",
    "image": "mcr.microsoft.com/vscode/devcontainers/base:ubuntu",
    "features": {
      "ghcr.io/devcontainers/features/node:1": {
        "version": "16"
      }
    },
    "postCreateCommand": "bash ./.devcontainer/setup.sh",
    "customizations": {
      "vscode": {
        "extensions": [
          "iden3.circom",
          "dbaeumer.vscode-eslint"
        ]
      }
    }
  }